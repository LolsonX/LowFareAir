<ol class="breadcrumb">
  <li class="breadcrumb-item active mr-auto">Cities</li>
</ol>
<div class="card-columns">
  <% @cities.each do |city| %>
    <div class="card card mb-4">
      <div class="card-body">
        <h5 class="card-title"><%= link_to city.to_s, city %></h5>
        <p class="card-text"><%= city.created_at %></p>
      </div>
    </div>
  <% end %>
</div>
