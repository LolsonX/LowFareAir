<ol class="breadcrumb">
  <li class="breadcrumb-item active mr-auto">Flight_histories</li>
</ol>
<div class="card-columns">
  <% @flight_histories.each do |flight_history| %>
    <div class="card card mb-4">
      <div class="card-body">
        <p class="card-title"><%= link_to flight_history.flight.to_s, flight_history %></p>
        <p class="card-text"></p>
      </div>
    </div>
  <% end %>
</div>
